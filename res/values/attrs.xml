<resources>

    <declare-styleable name="SeekBarPreference">
        <attr name="defaultVal" format="integer" />
        <attr name="maximum" format="integer" />
        <attr name="minimum" format="integer" />
        <attr name="unitsLeft" format="string|reference" />
        <attr name="unitsRight" format="string|reference" />
        <attr name="interval" format="integer" />
    </declare-styleable>

    <declare-styleable name="IntervalSeekBar">
        <attr name="min" format="float" />
        <attr name="max" format="float" />
        <attr name="defaultValue" format="float" />
        <attr name="digits" format="integer" />
    </declare-styleable>

    <attr name="switchBarTheme" format="reference" />
    <attr name="switchBarMarginStart" format="dimension" />
    <attr name="switchBarMarginEnd" format="dimension" />

</resources>
