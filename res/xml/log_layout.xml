<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <ListPreference
        android:key="aicp_log_share_type"
        android:title="@string/log_it_share_type_title"
        android:entries="@array/log_it_share_type_entries"
        android:entryValues="@array/log_it_share_type_values"
        android:defaultValue="1" />

    <CheckBoxPreference
        android:key="logcat"
        android:title="@string/logcat_title"
        android:defaultValue="false" />

    <CheckBoxPreference
        android:key="kmseg"
        android:title="@string/kmesg_title"
        android:defaultValue="false" />

    <CheckBoxPreference
        android:key="dmesg"
        android:title="@string/dmesg_title"
        android:defaultValue="false" />

    <Preference
        android:key="aicp_log_it"
        android:title="@string/log_it_title"
        android:summary="@string/log_it_summary" />

</PreferenceScreen>
